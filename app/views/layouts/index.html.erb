<h2>Kittens List</h2>
<table>
  <thead>
    <tr>
      <%# <th>Select</th> %>
      <th>Name</th>
      <th>Age</th>
      <th>Cuteness</th>
      <th>Softness</th>
    </tr>
  </thead>
<tbody>
  <% @kittens.each do |kitten| %>
    <tr>
      <td><%= kitten.name %></td>
      <td><%= kitten.age %></td>
      <td><%= kitten.cuteness %></td>
      <td><%= kitten.softness %></td>
    </tr>
  <%end%>
  </tbody>
</table>
